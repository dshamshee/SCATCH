    <!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link href="https://cdn.jsdelivr.net/npm/remixicon@4.2.0/fonts/remixicon.css" rel="stylesheet" />
    <script src="https://cdn.tailwindcss.com"></script>
    <link rel="stylesheet" href="/stylesheets/input.css">
    <link rel="stylesheet" href="https://unpkg.com/swiper/swiper-bundle.min.css" />
</head>

<body class="font-['helvetica_now_display']">
    <nav class="w-full flex justify-between px-5 py-3 bg-white shadow-md fixed top-0 left-0 z-50">
        <h3 class="text-xl"><strong class="text-2xl">S</strong>catch</h3>
        <% loggedin = typeof loggedin !== "undefined" ?  loggedin: true %>
        <% Showcart = typeof Showcart !== "undefined" ?  Showcart: true %>
        <% if(loggedin){ %>
            <div class="flex gap-5">
                <% if(Showcart){ %>
                    <a href="/shop">Shop</a>
                    <a href="/cart">Cart</a>
                    <a href="#">My Account</a>
                <% }else{ %>
                    <a href="/owners/showallproducts">Home</a>
                    <a href="/owners/profile">My Account</a>
                <% } %>
                <a class="text-red-600" href="/users/logout">Logout</a>
            </div>
        <% } %>
    </nav>
    <div class="pt-16">
        <!-- Content below navbar -->
    </div>
</body>

</html>


